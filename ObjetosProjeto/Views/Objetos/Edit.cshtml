@model ObjetosProjeto.Models.ObjetoModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>ObjetoModel</h4>
<hr />
<<h4>Inclusão de Objetos Achados & Perdidos</h4>
<hr />

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
    <label for="Objeto">O que você Encontrou / Perdeu?</label>
    <label asp-for="Objeto" class="control-label"></label>
    <input asp-for="Objeto" class="form-control" />
    <span asp-validation-for="Objeto" class="text-danger"></span>
</div>

<div class="form-group">
    <label for="Modelo">"Qual a Marca / Modelo?</label>
    <label asp-for="Modelo" class="control-label"></label>
    <input asp-for="Modelo" class="form-control" />
    <span asp-validation-for="Modelo" class="text-danger"></span>
</div>

<div class="form-group">
    <label for="Objeto">Quais os detalhes?</label>
    <label asp-for="Detalhes" class="control-label"></label>
    <input asp-for="Detalhes" class="form-control" />
    <span asp-validation-for="Detalhes" class="text-danger"></span>
</div>

<div class="form-group">
    <label for="Objeto">Onde você Encontrou / Perdeu?</label>
    <label asp-for="Localizacao" class="control-label"></label>
    <input asp-for="Localizacao" class="form-control" />
    <span asp-validation-for="Localizacao" class="text-danger"></span>
</div>

<div class="form-group">
    <label for="Objeto">Identificação</label>
    <label asp-for="ContatoId" class="control-label"></label>
    <input asp-for="ContatoId" class="form-label" />
</div>

<div class="form-group">
    <input type="submit" value="Adicionar" class="btn btn-primary" />
    <a class="btn btn-primary" asp-controller="Contato" asp-action="Index" role="button">  Voltar  </a>

</div>





@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
